<div class='secondinner'>

<%= form_for @book, :html => {:multipart => true} do |f| %>
<%= f.select :group_id, @groups, :include_blank => true %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_field :description %>




<%= f.select :chain_id, @chains, :include_blank => true %>



<%= f.select :author_id, @authors, :include_blank => true %>

<%= f.submit %>

<% end %>
</div>
